#ifndef __SGI_STL_NUMERIC
#define __SGI_STL_NUMERIC

#include <stl_config.h>

#if defined (__STL_USE_OWN_NAMESPACE)
# include __STL_NATIVE_HEADER(numeric)
#endif

#include <stl_relops.h>

# if defined (__STL_USE_NEW_STYLE_HEADERS)
# include <cstddef>
# else
# include <stddef.h>
# endif

# if defined (__STL_USE_NEW_IOSTREAMS) 
#  include <iostream>
# else
#  include  <iostream.h>   // for istream, ostream
# endif


#include <stl_iterator_base.h>
#include <stl_iterator.h>
#include <stl_function.h>
#include <stl_numeric.h>

#endif /* __SGI_STL_NUMERIC */

