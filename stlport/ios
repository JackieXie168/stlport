#ifndef __SGI_STL_IOS
#define __SGI_STL_IOS

# ifndef __STL_OUTERMOST_HEADER_ID
#  define __STL_OUTERMOST_HEADER_ID 0x1032
#  include <stl/_prolog.h>
# endif

# if defined ( __SGI_STL_OWN_IOSTREAMS )
#  include <stl/_ios.h>
# elif !defined (__STL_USE_NO_IOSTREAMS)
#  include <wrap_std/ios>
# endif

# if (__STL_OUTERMOST_HEADER_ID == 0x1032)
#  include <stl/_epilog.h>
#  undef __STL_OUTERMOST_HEADER_ID
# endif

#endif /* __SGI_STL_IOS */

